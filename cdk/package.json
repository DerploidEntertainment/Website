{
  "name": "github-pages-organization-website-cdk",
  "version": "0.1.0",
  "bin": {
    "github-pages-organization-website-cdk": "bin/github-pages-organization-website-cdk.js"
  },
  "scripts": {
    "compile": "tsc",
    "compile:watch": "tsc -w",
    "test": "jest",
    "synth": "cdk synth",
    "synth:prod": "NODE_ENV=prod npm run synth",
    "clean": "rm -rf lib/*.js lib/*.d.ts bin/*.js bin/*.d.ts",
    "diff": "cdk diff --app cdk.out/",
    "import": "cdk import --app cdk.out/",
    "deploy": "cdk deploy --app cdk.out/",
    "deploy-all": "npm run deploy -- --all"
  },
  "devDependencies": {
    "@types/jest": "^28.1.4",
    "@types/node": "10.17.27",
    "aws-cdk": "2.30.0",
    "jest": "^28.1.2",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.2",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "aws-cdk-lib": "2.30.0",
    "constructs": "^10.1.43",
    "source-map-support": "^0.5.21"
  }
}
