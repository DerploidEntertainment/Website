{
  "name": "github-pages-website-cdk",
  "version": "0.1.0",
  "bin": {
    "github-pages-website-cdk": "bin/github-pages-website-cdk.js"
  },
  "scripts": {
    "compile": "tsc",
    "compile:watch": "tsc -w",
    "test": "jest",
    "synth": "envFile=../.devcontainer/cdk/secret.$NODE_ENV.env && test -f \"$envFile\" && export $(grep -v '^#' \"$envFile\" | xargs -d '\\n'); cdk synth",
    "synth:test": "export NODE_ENV=test && npm run synth -- $@",
    "synth:prod": "export NODE_ENV=prod && npm run synth -- $@",
    "clean": "rm -rf lib/*.js lib/*.d.ts bin/*.js bin/*.d.ts",
    "diff": "cdk diff --app cdk.out/",
    "import": "cdk import --app cdk.out/",
    "deploy": "cdk deploy --app cdk.out/",
    "deploy:test": "npm run deploy -- $@",
    "deploy:prod": "npm run deploy -- $@"
  },
  "devDependencies": {
    "@types/jest": "^28.1.8",
    "@types/node": "10.17.27",
    "aws-cdk": "2.33.0",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.2",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "aws-cdk-lib": "2.33.0",
    "constructs": "^10.4.2",
    "source-map-support": "^0.5.21"
  }
}
